{
  "name": "faux-mysql-server",
  "private": false,
  "version": "0.1.0",
  "description": "A library which implements the MySQL server protocol, giving you the ability to create MySQL-like services",
  "keywords": [ "mysql" ],
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-minimal": "^2.1.0",
    "babel-preset-stage-0": "^6.16.0"
  },
  "scripts": {
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node --harmony dist",
    "prestart": "npm run -s build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/CloudQuote/faux-mysql-server.git"
  },
  "author": "mdierolf",
  "license": ""
}
